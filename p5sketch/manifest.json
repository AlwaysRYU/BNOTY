{
  "manifest_version": 2,
  "name": "p5 sketch",
  "version": "1.0",
  "icons": {
    "128": "./images/image128.png",
    "48": "./images/image48.png",
    "16": "./images/image16.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["p5.js", "p5.min.js", "sketch.js"]
    }
  ]
}
